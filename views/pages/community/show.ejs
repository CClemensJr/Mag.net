<!DOCTYPE html>
<html>
        <%- include('../../partials/head') %>
<body>
        <%- include('../../partials/header') ; %>
 <main>
        <% for(let i = 0; i < data.length; i++) { %>
                <% for(let j = 0; j < data[i].length; j++) { %>
                        <% if (i === 0) { %>
                                <p class="letterMag" style="position: absolute; left: <%= data[i][j].x + 20 %>px; top: <%= data[i][j].y + 20 %>px; color: red;"><%= data[i][j].content %></p>
                        <% } else if (i === 1) { %>
                                <img class="memeMag" src=<%= data[i][j].content %> alt="" height="100" width="100">
                        <% } else { %>
                                <p class="wordMag" display><%= data[i][j].content %></p>

                        <% } %>
                <% } %>
        <% } %>
     <figure>
            <img id="fridgeImg" src =  "img/fridges.png ">

     </figure>

 </main>
        <%- include('../../partials/footer'); %>

</body>

</html>